{
    "source": {
      "include": ["src"],  
      "includePattern": ".+\\.js$", 
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
      "destination": "./docs",  
      "recurse": true
    },
    "plugins": [
      "plugins/markdown"
    ],
    "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
    }
  }